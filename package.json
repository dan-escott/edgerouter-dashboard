{
  "name": "edgerouter-dashboard",
  "version": "0.0.0-development",
  "private": true,
  "homepage": "/dashboard",
  "dependencies": {
    "@dan-escott/react-dashboard": "^2.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.19.16",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-tabs": "^2.3.2",
    "chart.js": "^2.9.4",
    "chartjs-chart-treemap": "^0.2.3",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.2",
    "react-tabs": "^3.2.1",
    "typescript": "^4.1.3",
    "web-vitals": "^1.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint . --ext .tsx,.ts,.js,.jsx",
    "eject": "react-scripts eject",
    "semantic-release": "semantic-release",
    "zip": "npm-build-zip --source=build --destination=build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "prettier": "^2.2.1",
    "semantic-release": "^17.3.9"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "assets": [
            {
              "path": "edgerouter-dashboard.tar.gz",
              "name": "edgerouter-dashboard-${nextRelease.gitTag}.tar.gz",
              "label": "EdgeRouter Dashboard"
            }
          ]
        }
      ]
    ],
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "main",
      "next",
      "next-major",
      {
        "name": "beta",
        "prerelease": true
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ]
  }
}
